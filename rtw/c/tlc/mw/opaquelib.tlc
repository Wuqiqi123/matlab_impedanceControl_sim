    %assign expr = SLibCG_V(expr, TLC_FALSE, width, strEl, chunkIdx, ucv, sigIdx)
  %endif
  %assign ::CompiledModel.GeneratingOutputsCode = prevGeneratingOutputsCode
  %return expr
%endfunction

