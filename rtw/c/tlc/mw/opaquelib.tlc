%function SLibCG_DumpSSCode(openComment, ssBody, closeComment) void
  %if WHITE_SPACE(ssBody)
    %return ""
  %else
    %if TYPE(ssBody) == "String"
      %return openComment + ssBody + closeComment
    %else
