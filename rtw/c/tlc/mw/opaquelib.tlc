  %if -1 == parentVarGroupIdx
    %return TLC_FALSE
  %endif
  %assign parentVarGroup = ::CompiledModel.VarGroups.VarGroup[parentVarGroupIdx]
  %return 1 == parentVarGroup.PackageAsStruct
%endfunction

