    "(void * destData, const mxArray * srcArray, mwIndex i, int j, mwIndex offset, size_t numBytes)"
  %assign fcnSig  = ...
    "static void %<SLibGetRestoreDataFromMxArrayWithOffsetFcnName()>%<fcnArgs>"

