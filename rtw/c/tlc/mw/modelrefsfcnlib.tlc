    extern %<qualifiers> %<FcnSysVarGroupType(System[NumSystems-2],"ConstBlockIO")>  %<::tConstBlockIOStruct>;
  %endif
  %closefile extCBIOExtern
  %return extCBIOExtern
%endfunction %% GetConstBIOExternBuf


