        %% error status, need explicity cast to
        %% avoid compiler warning.
        %assign accStr = "((const char_T **)%<accStr>)"
      %endif
    %else
      %assign accStr = ""
    %endif
  %else
    %assign accStr = "" %% Undefined
  %endif

  %return accStr
