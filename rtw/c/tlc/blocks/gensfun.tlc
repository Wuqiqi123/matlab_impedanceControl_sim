	    %assign s = ::tChildSimStruct
	    {
              SimStruct *%<s> = %<RTMGetIdxed("SFunction", childSFunctionIdx)>;

              %assign methodName = "sfcnEnable"
                %if ::isRAccel || IsModelReferenceSimTarget()
                  %assign actualFcnCall = ...
