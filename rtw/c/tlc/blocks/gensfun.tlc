	    }
	  %endif
	%endif
      %endif
      %if ParamSettings.ExplicitFCSSCtrl == "yes"
        %<InvokeFcnCallForTracking(block, "Enable")>
      %endif
  %endswitch
  %closefile buffer
  %if !ISEMPTY(buffer) && !WHITE_SPACE(buffer)
    /* %<TypeLevel> Block: '%<Name>' (%<ParamSettings.FunctionName>) */
